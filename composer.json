{
  "name": "symfony/framework-standard-edition",
  "license": "MIT",
  "type": "project",
  "description": "The \"Symfony Standard Edition\" distribution",
  "autoload": {
    "files": ["src/Kernel.php"],
    "psr-4": {
      "Log\\": "src/"
    }
  },
  "require": {
    "symfony/console": "^5.4",
    "symfony/dependency-injection": "^5.4",
    "symfony/config": "^5.4",
    "symfony/yaml": "^5.4"
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "prefer-stable" : true,
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
